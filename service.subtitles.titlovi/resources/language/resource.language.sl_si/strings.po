msgid "Features"
msgstr "Funkcije"

msgctxt "#32301"
msgid "Subtitle Conversion"
msgstr "Pretvorba podnapisov"

msgctxt "#32302"
msgid "Convert latin/cyrillic alphabet"
msgstr "Pretvori latinično/cirilično pisavo"

msgctxt "#32303"
msgid "Automatically convert between Serbian Latin and Cyrillic subtitles when needed, except for English subtitles."
msgstr "Samodejno pretvori med srbskimi latiničnimi in ciriličnimi podnapisi po potrebi, razen za angleške podnapise."

